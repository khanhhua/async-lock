{
  "presets": [
    ["env", {
      "targets": {
        "node": "current"
      }
    }]
  ],
  "ignore": "node_modules/*",
  "plugins": [
    "transform-class-properties"
  ],
  "env": {
    "test": {
      "plugins": [ "istanbul", {
        "exclude": [
          "**/*.spec.js",
          "**/*.test.js"
        ]
      }]
    }
  }
}
